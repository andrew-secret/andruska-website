{
  "name": "andruska-website",
  "description": "Andruska portfolio website with contentful and graphQL",
  "version": "1.0.0",
  "author": "Andr√© Rusakow <andre.rusakow@googlemail.com>",
  "dependencies": {
    "@types/classnames": "^2.2.3",
    "@types/react-helmet": "^5.0.6",
    "classnames": "^2.2.5",
    "dotenv": "^5.0.1",
    "gatsby": "^1.9.73",
    "gatsby-link": "^1.6.22",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-sass": "^1.0.12",
    "gatsby-plugin-typescript": "^1.4.20",
    "gatsby-source-contentful": "^1.3.48",
    "material-design-icons": "^3.0.1",
    "react": "^16.3.2",
    "react-dom": "^16.0.0",
    "typescript": "^2.8.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "mjs",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ]
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "test": "jest",
    "deploy": "gatsby build --prefix-paths -d public"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/react-test-renderer": "^16.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.3",
    "prettier": "^1.7.4",
    "react-test-renderer": "^16.3.2",
    "ts-jest": "^22.4.6"
  }
}
